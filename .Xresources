Xft.dpi: 192
Xft.antialias: 1
Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.rgba: rgb

! urxvt settings
!URxvt.font: xft:SourceCodePro:style=Medium:size=11
URxvt.font: xft:DMMono:size=11
URxvt.scrollBar: false
URxvt.internalBorder: 20
URxvt.externalBorder: 0

! special
*.foreground:   #e9ddde
*.background:   #040102
