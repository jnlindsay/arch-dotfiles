! black
*.color0:       #070303
*.color8:       #b8a37e

! red
*.color1:       #1d0f11
*.color9:       #99c4bf

! green
*.color2:       #351f22
*.color10:      #719591

! yellow
*.color3:       #56343a
*.color11:      #d69ddf

! blue
*.color4:       #805058
*.color12:      #6c9662

! magenta
*.color5:       #b1717c
*.color13:      #978769

! cyan
*.color6:       #c9a7ac
*.color14:      #8dcb7e

! white
*.color7:       #ebe0e1
*.color15:      #c96dd5
